<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <!-- swe → nor -->

  <mode name="swe-nob" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="swe-nob.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="swe-nor.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="swe-nor.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="swe-nob.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-swe-nor.swe-nor.t1x"/>
        <file name="swe-nor.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-swe-nor.swe-nor.t2x"/>
        <file name="swe-nor.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-swe-nor.swe-nor.t3x"/>
        <file name="swe-nor.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="swe-nob.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="swe-nno" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="swe-nno.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="swe-nor.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="swe-nor.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="swe-nno.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-swe-nor.swe-nor.t1x"/>
        <file name="swe-nor.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-swe-nor.swe-nor.t2x"/>
        <file name="swe-nor.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-swe-nor.swe-nor.t3x"/>
        <file name="swe-nor.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="swe-nno.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <!-- nor → swe -->

  <mode name="nob-swe" install="yes">
    <pipeline>
      <program name="lt-proc -w -e">
        <file name="nob-swe.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="nob-swe.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="nob-swe.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="nor-swe.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-swe-nor.nor-swe.t1x"/>
        <file name="nor-swe.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-swe-nor.nor-swe.t2x"/>
        <file name="nor-swe.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-swe-nor.nor-swe.t3x"/>
        <file name="nor-swe.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="nor-swe.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="nno-swe" install="yes">
    <pipeline>
      <program name="lt-proc -w -e">
        <file name="nno-swe.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
